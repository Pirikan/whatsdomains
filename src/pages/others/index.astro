---
import BaseLayout from '../../layouts/BaseLayout.astro';
import OthersPost from '../../components/OthersPost.astro';
const allOthers = await Astro.glob('../../pages/others/*/index.astro');
const pageTitle = "Others";
---

<BaseLayout pageTitle={pageTitle}>
    
    <ul class="index-link-cards">
        {allOthers.map((post) => <OthersPost post={post} />)}
    </ul>
</BaseLayout>